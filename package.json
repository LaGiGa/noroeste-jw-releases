{
  "name": "noroeste",
  "private": true,
  "version": "1.1.0",
  "description": "Sistema de Gerenciamento para Congregação Noroeste",
  "author": "Congregação Noroeste",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/LaGiGa/noroeste-jw-releases.git"
  },
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && cross-env ELECTRON_IS_DEV=true electron .\"",
    "electron:build": "npm run build && npx electron-builder",
    "electron:package": "npm run build && npx electron-builder --dir && powershell -ExecutionPolicy Bypass -File ./create-installer.ps1",
    "electron:publish": "npm run build && npx electron-builder --publish always"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@react-google-maps/api": "^2.20.7",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/supabase-js": "^2.47.10",
    "@types/uuid": "^10.0.0",
    "bootstrap": "^5.3.8",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.6.2",
    "html2canvas": "^1.4.1",
    "jsdom": "^27.3.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lowdb": "^7.0.1",
    "lucide-react": "^0.555.0",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.10.38",
    "puppeteer": "^24.34.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "supabase": "^2.67.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}